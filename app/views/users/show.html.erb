<div class='row'>
  <h1><%= @user.first_name %> <%= @user.last_name %></h1>
  <p id="notice"><%= notice %></p>

  <table>
    <tr>
      <th>Email:</th>
      <td><%= @user.email %></td>
    </tr>
    <tr>
      <th>Username:</th>
      <td><%= @user.name %></td>
    </tr>
    
  </table><br></br>
  <%= link_to 'Edit', edit_user_path(@user) %> |
  <%= link_to 'Back', '/' %><br><br>
</div>